<div class="col-sm-8 col-lg-9">
    <div class="content">
        <!-- List Vendors -->
        <div class="cards-row">
            {% for row in category.vendors %}
                <div class="card-row">
                    <div class="card-row-inner">
                        <div class="card-row-image" data-background-image="{{ asset('assets/img/tmp/banner.jpg') }}" >
                            <div class="card-row-label"><a href="listing-detail.html">{{ category.Name }}</a></div><!-- /.card-row-label -->
                            <div class="card-row-price">x</div><!-- -->
                        </div><!-- /.card-row-image -->

                        <div class="card-row-body">
                            <h2 class="card-row-title"><a href="listing-detail.html">{{ row.name }}</a></h2>
                            <div class="card-row-content"><p>{{ row.website }}</p></div><!-- /.card-row-content -->
                        </div><!-- /.card-row-body -->

                        <div class="card-row-properties">
                            <dl>
                                <dd>Price</dd><dt>x</dt>

                                <dd>Category</dd><dt>x</dt>

                                <dd>Location</dd><dt></dt>

                                <dd>Rating</dd>

                                <dt>
                                    <div class="card-row-rating">
                                        <i class="fa fa-star"></i> <i class="fa fa-star"></i> <i class="fa fa-star"></i> <i class="fa fa-star"></i> <i class="fa fa-star"></i>
                                    </div><!-- /.card-row-rating -->
                                </dt>
                            </dl>
                        </div><!-- /.card-row-properties -->
                    </div><!-- /.card-row-inner -->
                </div><!-- /.card-row -->
            {% endfor %}
        </div><!-- /.cards-row -->

        <!-- Pagination -->
        <div class="pager">
            <ul>
                <li><a href="#">Prev</a></li>
                <li><a href="#">5</a></li>
                <li class="active"><a>6</a></li>
                <li><a href="#">7</a></li>
                <li><a href="#">Next</a></li>
            </ul>
        </div><!-- /.pagination -->
    </div><!-- /.content -->
</div><!-- /.col-* -->