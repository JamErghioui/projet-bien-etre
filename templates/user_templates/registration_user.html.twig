<div class="page-title">
    <h1>New User</h1>
</div><!-- /.page-title -->
{{ form_start(RegistrationUserForm) }}
<div class="form-group">
    {{ form_row(RegistrationUserForm.username, { 'attr' : { 'class' : 'form-control' }}) }}
</div>
<div class="form-group">
    {{ form_row(RegistrationUserForm.email, { 'attr' : { 'class' : 'form-control' }}) }}
</div>
<div class="form-group">
    {{ form_row(RegistrationUserForm.password, { 'attr' : { 'class' : 'form-control' }}) }}
</div>
<div class="form-group">
    {{ form_row(RegistrationUserForm.confirm_password, { 'attr' : { 'class' : 'form-control' }}) }}
</div>
<div class="form-group">
    <button type="submit" class="btn btn-primary pull-right">Register</button>
</div>
{{ form_end(RegistrationUserForm) }}