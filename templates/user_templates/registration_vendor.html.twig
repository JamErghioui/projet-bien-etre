<div class="page-title">
    <h1>New Vendor</h1>
</div><!-- /.page-title -->
{{ form_start(RegistrationVendorForm) }}
<div class="form-group">
    {{ form_row(RegistrationVendorForm.username, { 'attr' : { 'class' : 'form-control' }}) }}
</div>
<div class="form-group">
    {{ form_row(RegistrationVendorForm.email, { 'attr' : { 'class' : 'form-control' }}) }}
</div>
<div class="form-group">
    {{ form_row(RegistrationVendorForm.password, { 'attr' : { 'class' : 'form-control' }}) }}
</div>
<div class="form-group">
    {{ form_row(RegistrationVendorForm.confirm_password, { 'attr' : { 'class' : 'form-control' }}) }}
</div>
<div class="form-group">
    <button type="submit" class="btn btn-primary pull-right">Register</button>
</div>
{{ form_end(RegistrationVendorForm) }}